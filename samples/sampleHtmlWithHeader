11dd9876f446780b7bdc&quot;,&quot;CurrentPosition&quot;:1,&quot;CurrentWatsonAnswerText&quot;:null,&quot;DocAnalyzerReport&quot;:null,&quot;DualNavigationData&quot;:null,&quot;DualRelatedDocsData&quot;:null,&quot;EnableConceptCounts&quot;:false,&quot;FermiSearchResult&quot;:&quot;i0a8991e700000197d73f5fd26efe6b2b&quot;,&quot;HighlightPoints&quot;:null,&quot;HighlightTerms&quot;:[&quot;fails&quot;,&quot;distinguish&quot;,&quot;texas&quot;,&quot;constitution&quot;,&quot;fourth&quot;,&quot;amendment&quot;],&quot;InPlanUrl&quot;:null,&quot;IsSnapSnippet&quot;:false,&quot;JudicialReport&quot;:null,&quot;ListCount&quot;:2,&quot;Message&quot;:null,&quot;NextDocument&quot;:{&quot;Current&quot;:false,&quot;Guid&quot;:&quot;I8d225121e7df11d99439b076ef9ec4de&quot;,&quot;LongTitle&quot;:null,&quot;OriginalDocumentGuid&quot;:null,&quot;SkipLinkOutDoc&quot;:false,&quot;Title&quot;:&quot;Cardenas v. State&quot;,&quot;Url&quot;:&quot;/Document/I8d225121e7df11d99439b076ef9ec4de/View/FullText.html?navigationPath=Search%2Fv1%2Fresults%2Fnavigation%2Fi0a8991e700000197d73f5fd26efe6b2b%3FNav%3DCASE%26fragmentIdentifier%3DI6cdab4fa705011dd9876f446780b7bdc%26startIndex%3D1%26contextData%3D%2528sc.Search%2529%26transitionType%3DSearchItem%26ppcid%3D56410806badd49b296cc3859162c2ced&amp;listSource=Search&amp;list=CASE&amp;rank=2&amp;grading=na&amp;sessionScopeId=42667fd9d6b5cc6c66ef8a1fefe0cfe6cd41717210851dbf7c76a87f52ec02c2&amp;ppcid=56410806badd49b296cc3859162c2ced&amp;originationContext=previousnextdocument&amp;transitionType=SearchItem&amp;contextData=%28sc.Search%29&amp;listPageSource=d926b40fa532d6cdd3dbe2af65886760&quot;},&quot;NormalizedSearchQuery&quot;:&quot;\&quot;fails to distinguish\&quot; \&quot;texas constitution\&quot; and \&quot;fourth amendment\&quot;&quot;,&quot;NovusQueryType&quot;:&quot;SEARCH&quot;,&quot;NovusSearchResult&quot;:&quot;i0a89aba100000197d73f614e61e4f1c2&quot;,&quot;NumberOfSnips&quot;:4,&quot;OriginalQuery&quot;:&quot;adv: \&quot;fails #to distinguish\&quot